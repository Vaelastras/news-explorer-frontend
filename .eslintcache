ticleId) => fetch(\n  `${BASE_URL}/articles/${articleId._id}`,\n  {\n    method: 'DELETE',\n    headers: {\n      'Content-Type': 'application/json',\n      Authorization: `Bearer ${localStorage.getItem('jwt')}`,\n    },\n  },\n).then((res) => res.json())\n  .catch((err) => Promise.reject(err.message));\n\n// сохранить карту в своем апи\nexport const saveArticle = (article, keyword) => {\n  const {\n    title,\n    description,\n    publishedAt,\n    source,\n    url,\n    urlToImage,\n  } = article;\n  return fetch(\n    `${BASE_URL}/articles`,\n    {\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/json',\n        Authorization: `Bearer ${localStorage.getItem('jwt')}`,\n      },\n      body: JSON.stringify({\n        keyword,\n        title,\n        text: description || title,\n        date: publishedAt,\n        source: source.name,\n        link: url,\n        image: urlToImage || newsDefaultImage,\n      }),\n    },\n  )\n    .then((res) => res.json())\n    .catch((err) => Promise.reject(err.message));\n};\n","/Users/vaelastras/Yandex/news-explorer-frontend/src/utils/NewsApi.js",[],"/Users/vaelastras/Yandex/news-explorer-frontend/src/utils/constants.js",["126"],"const BASE_URL = `${window.location.protocol}${process.env.REACT_APP_API_URL || '//localhost:3000'}`;\nconst API_KEY = '8c43c8fd4f0c449e95267fecb55fb71b';\nconst PROXY_URL = 'https://nomoreparties.co/news/v2/everything';\nconst CARDS_TO_SHOW = 3;\nconst DAY_DECREMENT = 7;\nconst COUNT_NEWS_SHOWED = 20;\n\nmodule.exports = {\n  BASE_URL,\n  API_KEY,\n  PROXY_URL,\n  CARDS_TO_SHOW,\n  DAY_DECREMENT,\n  COUNT_NEWS_SHOWED,\n};\n",{"ruleId":"127","replacedBy":"128"},{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","severity":1,"message":"140","line":152,"column":15,"nodeType":"141","messageId":"142","endLine":152,"endColumn":26},{"ru